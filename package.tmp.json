{
  "name": "umpui-init",
  "version": "1.0.0",
  "description": "ump ui frame",
  "main": "index.js",
  "scripts": {
    "cp-webpack-config": "cp ../frontend/umpui-react/webpack.config.tmp.js ./webpack.config.js",
    "create-project-frontend-dir": "mkdir dist && mkdir resources && cd dist && mkdir js && mkdir css  && cd ../resources && mkdir jsx && mkdir sass && mkdir views && cd ../",
    "umpui-css": "cp node_modules/umpui-react/dist/css/common3.min.css dist/css/common.min.css",
    "antd-css": "cp node_modules/umpui-react/dist/css/antd.css dist/css/antd.css",
    "copy-example-file": "cp node_modules/umpui-react/examples/demo.php resources/views/",
    "copy-example-js": "cp node_modules/umpui-react/examples/demo.js resources/jsx/",
    "copy-bundle-js": "cp node_modules/umpui-react/dist/js/demo.bundle.js dist/js/",
    "init-project": "npm run cp-webpack-config && npm run create-project-frontend-dir && npm run umpui-css && npm run antd-css && npm run copy-example-file && npm run copy-example-js && npm run copy-bundle-js"
  },
